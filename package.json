    "ssr": "bun run ssr:build && bun run ssr:run"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "preact": "^10.19.3",
    "preact-async-route": "^2.2.1",
    "preact-render-to-string": "^6.3.1",
    "preact-router": "^4.1.2",
    "redis": "^4.6.13",
    "serve-static": "^1.15.0",
    "vite-plugin-compression2": "^0.12.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.1",
    "@types/bun": "^1.0.6",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
