{
  "name": "moscode-frontend-preact-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "ssr:tailwind": "bun run",
    "ssr:build": "vite build --outDir ./dist/client && vite build --outDir ./dist/server --ssr ./src/entry-server.tsx",
